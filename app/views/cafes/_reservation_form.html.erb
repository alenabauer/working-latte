<%= simple_form_for [chair, reservation], url: chair_reservations_path(chair) do |f| %>
  <%= f.input :date, as: :date %>
  <%= f.association :time_slots, collection: chair.time_slots, label_method: :to_label, value_method: :id, as: :check_boxes %>
  <%= f.submit "Book!", class: "form-submit-btn" %>
<% end %>
